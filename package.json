  "devDependencies": {
    "@stencil/dev-server": "latest",
    "@stencil/utils": "latest",
    "@types/jest": "^21.1.1",
    "jest": "^21.2.1"
  },
  "prettier": {
    "semi": true,
    "printWidth": 170,
    "singleQuote": true
  }
